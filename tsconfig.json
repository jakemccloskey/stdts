{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": true,
        "sourceMap": true,
        "outDir": "./target",
        "moduleResolution": "node",
        "allowJs": true,
        "noEmitOnError": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "pretty": true,
        "strictNullChecks": true
    },
    "exclude": [
        "node_modules"
    ]
}
