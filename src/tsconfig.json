{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "noEmitOnError": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "pretty": true,
        "strictNullChecks": true,
        "outDir": "../dist",
        "allowJs": true,
        "lib": [
            "es2015"
        ]
    },
    "exclude": [
        "node_modules",
        "test"
    ]
}
